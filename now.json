{
  "version": 2,
  "builds": [
    { "src": "wp-config.php", "use": "@now/wordpress" }
  ],
  "routes": [
    { "src": "/wp-admin/?", "dest": "index.php" },
    { "src": ".*\\.php$", "dest": "index.php" }
  ],
  "env": {
    "WP_ENV": "dev",
    "DB_NAME": "@pb_wp_db_dev_name",
    "DB_USER": "@pb_wp_db_dev_user",
    "DB_PASSWORD": "@pb_wp_db_dev_password",
    "DB_HOST": "@pb_wp_db_dev_host",
    "WP_SALT": "@pb_wp_dev_salt",
    "S3_KEY": "@pb_wp_s3_key",
    "S3_SECRET": "@pb_wp_s3_secret"
  },
  "regions": ["iad1"]
}
